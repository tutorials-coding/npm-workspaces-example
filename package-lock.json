{
  "name": "npm-workspaces-example",
  "version": "1.0.0",
  "lockfileVersion": 2,
  "requires": true,
  "packages": {
    "": {
      "name": "npm-workspaces-example",
      "version": "1.0.0",
      "license": "Apache-2.0",
      "workspaces": [
        "packages/components",
        "packages/app",
        "packages/api"
      ]
    },
    "node_modules/app": {
      "resolved": "packages/app",
      "link": true
    },
    "node_modules/components": {
      "resolved": "packages/components",
      "link": true
    },
    "node_modules/sdk": {
      "resolved": "packages/api",
      "link": true
    },
    "packages/api": {
      "name": "sdk",
      "version": "1.0.0",
      "license": "Apache-2.0",
      "devDependencies": {}
    },
    "packages/app": {
      "version": "1.0.0",
      "license": "Apache-2.0"
    },
    "packages/components": {
      "version": "1.0.0",
      "license": "Apache-2.0"
    }
  },
  "dependencies": {
    "app": {
      "version": "file:packages/app"
    },
    "components": {
      "version": "file:packages/components"
    },
    "sdk": {
      "version": "file:packages/api"
    }
  }
}
